#lang racket
; https://github.com/dvanhorn/oaam/blob/master/benchmarks/sergey/blur.sch

(define id (位 (x) x))
(define blur (位 (y) y))
(define lp
  (位 (a)
    (位 (n)
      (if (zero? n)
	  (id a)
	  (let* ([r ((blur id) #t)]
		 [s ((blur id) #f)])
	    (not (((blur lp) s) (sub1 n))))))))

((lp #f) 2)
